@model List<Contact>;
@{
    ViewData["Title"] = "Home Page";
}

<a href="/">Add Contact</a>
<table class="table">
    <thead>
        <tr>
            <th> First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var contact in Model)
        {
            <tr>
                <td>@contact.F_Name</td>
                <td>@contact.L_Name</td>
                <td>@contact.PhoneNumber</td>
                <td>@contact.Category.Name</td>
                <td>
                    <a href="/">Edit</a>
                    <a href="/">Delete</a>
                </td>
            </tr>
        }

    </tbody>

</table>
